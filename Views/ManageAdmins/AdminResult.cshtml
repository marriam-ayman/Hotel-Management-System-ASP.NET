@model List<Admin>

<!-- Page Header Start -->

<div class="container-fluid page-header mb-5 p-0"
     style="background-image: url(../img/carousel-1.jpg)">
    <div class="container-fluid page-header-inner py-5">
        <div class="container text-center pb-5">
            <h1 class="display-3 text-white mb-3 animated slideInDown">
               Admins
            </h1>
            <nav aria-label="breadcrumb">
                <ol class="breadcrumb justify-content-center text-uppercase">
                    <li class="breadcrumb-item"><a href="#">Home</a></li>
                    <li class="breadcrumb-item text-white active"
                        aria-current="page">
                        Admins
                    </li>
                </ol>
            </nav>
        </div>
    </div>
</div>
<!-- Page Header End -->
<!-- Admin Start -->
<div class="container-xxl py-5">
    <div class="container">
        <div class="row g-5 mt-5">
            <div class="m-auto col-lg-6">
                <div class="wow fadeInUp" data-wow-delay="0.2s">
                  
                        <div class="row g-3">
                            <table>
                                <thead>
                                    <tr>
                                        <th>Admin ID</th>
                                        <th>Username</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (Admin admin in Model)
                                    {
                                        <tr>
                                            <td>@admin.ID</td>
                                            <td>@admin.Username</td>
                                            <td>
                                                <a class="btn btn-primary py-3 px-5 mt-2" asp-controller="ManageAdmins" asp-action="RemoveAdmin" asp-route-id="@admin.ID">Delete</a>
                                            </td>
                                        </tr>
                                        // Console.WriteLine($"Id: {admin.ID}, -Username: {admin.UserName}");
                                    }
                                </tbody>

                            </table>

                        </div>
                 
                </div>
            </div>
        </div>

    </div>
</div>
<!-- Admin End -->